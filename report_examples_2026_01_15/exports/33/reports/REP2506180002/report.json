  "custom_texts": [
    {
      "id": "4e41b8ef-881b-4919-9e1a-02d215c87526",
      "order": 1,
      "text": "Některé senzory vykazují zvýšenou vlhkost ve stěnách, která je způsobená vazbou na vyšší vlhkost v interiérových obytných místnostech.\r\n\r\n",
      "report_template_text_section_id": "5919f72e-8a0c-4d25-afe0-945bc1655696",
      "created_at": "2025-06-18 07:41:04 UTC",
